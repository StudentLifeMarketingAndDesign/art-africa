{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.5",
        "silverstripe/framework": "4.3.3",
        "silverstripe/cms": "4.3.3",
        "silverstripe/recipe-plugin": "*",
        "silverstripe/recipe-cms": "*",
        "silverstripe/siteconfig": "*",
        "undefinedoffset/sortablegridfield": "*",
        "colymba/gridfield-bulk-editing-tools": "*",
        "jonom/focuspoint": "*",
        "silverstripe/fulltextsearch": "*",
        "silverstripe/googlesitemaps": "*",
        "silverstripe/tagfield": "*",
        "md/division-analytics": "*",
        "md/uiowa-bar": "*"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-gridfield-betterbuttons"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-activedirectory"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/division-analytics"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/uiowa-bar"
        }
    ],
    "extra": {
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ],
         "expose": [
          "themes/africa/dist"
        ]
    }
}
